@using InTheBoks.Resources

@{
    ViewBag.Title = "InTheBoks.com - Organize Your Stuff";
}

<div class="thumbnails" data-bind="foreach: Items">


    <div class="thumbnail_container">
        <div class="wrapper" data-bind="click: $parent.SelectItem">   
            <img data-bind="attr: { src: ImageUrl, title: Title }" src="../../Content/albums/41A-jqYmLiL.jpg" />  
            <div class="description">  
            <p class="description_content"><strong data-bind="text: Title"></strong></p>  
            </div>  
        </div>  
        <span class="list_details"><strong data-bind="text: Title"></strong></span>
        <span class="list_details right" data-bind="text: Title"></span>
    </div>

</div>
@section scripts
{
<script>

    var mainVM = null;

    function InitializeUserData() {

        if (mainVM == null) {
            mainVM = new mainViewModel();
            ko.applyBindings(mainVM, document.getElementById("AppContainer"));
        }

        mainVM.LoadData();
    }

</script>

    
}